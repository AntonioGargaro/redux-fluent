{
  "name": "redux-fluent",
  "version": "0.0.1",
  "description": "Thiny and Eloquent way to manage (redux) Actions",
  "main": "build/redux-fluent.js",
  "scripts": {
    "test": "npm run jest",
    "test:watch": "npm run jest:watch",
    "webpack": "webpack --config webpack.config.js",
    "webpack:watch": "npm run webpack -- --watch",
    "start": "npm run webpack:watch",
    "build": "NODE_ENV=production npm run webpack",
    "jest": "jest --coverage",
    "jest:watch": "npm run jest -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Code-Y/redux-fluent.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Code-Y/redux-fluent/issues"
  },
  "homepage": "https://github.com/Code-Y/redux-fluent#readme",
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "jest": "^21.1.0",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.2",
    "webpack": "^3.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
